	<queryString>
		<![CDATA[SELECT E.descricao atividade,EA.descricao area,ET.descricao tipo FROM TAP.TAP_AADSP_ESCOPO E
INNER JOIN TAP.TAP_AADSP_ESCOPO_AREA EA
ON E.ID_escopoArea = EA.ID
INNER JOIN TAP.TAP_AADSP_ESCOPO_TIPO ET
ON E.ID_escopoTipo = ET.ID
WHERE E.ID_tap = $P{ID_tap}
GROUP BY E.descricao,EA.descricao,ET.descricao,ET.ID
ORDER BY ET.ID]]>
	</queryString>
