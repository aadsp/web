	<queryString>
		<![CDATA[SELECT 
ID_eap,
UPPER(T.nome) titulo,
ep.dataCadastro,
ti.descricao tipo
FROM PROJETO.PROJETO_AADSP_DIAGRAMA_EAP EP
INNER JOIN PROJETO.PROJETO_AADSP_PROJETO P
ON EP.ID_projeto = P.ID_projeto
INNER JOIN TAP.TAP_AADSP_TAP T
ON T.ID_tap = P.ID_tap
INNER JOIN PROJETO.PROJETO_AADSP_DIAGRAMA_EAP_TIPO TI
ON TI.ID_eapTipo = EP.ID_eapTipo
WHERE EP.ID_eap = $P{ID_eap}]]>
	</queryString>
